# Definition for singly-linked list.
class ListNode(object):
    def __init__(self, x):
        self.val = x
        self.next = None

class Solution(object):
    def hasCycle(self, head):
        temp = head 
        visited = set()
        while temp != None:
            if temp in visited :
                return True 
            visited.add(temp)
            temp = temp.next 
        return False 
            
        
