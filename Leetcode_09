class Solution(object):
    def isPalindrome(self, x):
        if x < 0:
            return False      # for negitive values 
        x = str(x)            # converting to str because len() is applied to strings not for int values 
        for i in range(len(x)):     
            if x[i] != x[len(x)-i-1]:
                return False
        return True
